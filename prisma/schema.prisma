generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model bloksensus {
  no_bs        String    @id(map: "pk_bloksensus") @db.VarChar(4)
  nama_sls     String    @db.VarChar(255)
  id_kab       String    @db.VarChar(3)
  id_kec       String    @db.VarChar(3)
  id_kelurahan String    @db.VarChar(3)
  jml_rt       Int       @default(0)
  jml_rt_genz  Int       @default(0)
  jml_genz     Int       @default(0)
  nim_pencacah String    @db.VarChar(9)
  tim_pencacah String    @db.VarChar(9)
  tgl_listing  DateTime? @db.Date
  tgl_periksa  DateTime? @db.Date
  catatan      String?   @db.VarChar(255)
  status       String    @db.VarChar(20)
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model datast {
  no_bs     String @db.VarChar(14)
  kode_ruta String @id(map: "pk_datast") @db.VarChar(30)
  status    String @db.VarChar(20)

  @@ignore
}

model kabupaten {
  id_kab   String @id(map: "pk_kabupaten") @db.VarChar(3)
  nama_kab String @db.VarChar(255)
}

model kecamatan {
  id_kec   String @id(map: "pk_kecamatan") @db.VarChar(3)
  nama_kec String @db.VarChar(255)
}

model kelurahan {
  id_kelurahan   String @id(map: "pk_kelurahan") @db.VarChar(3)
  nama_kelurahan String @db.VarChar(255)
}

model mahasiswa {
  nim            String @id(map: "pk_mahasiswa") @db.VarChar(9)
  email          String @db.VarChar(50)
  nama           String @db.VarChar(255)
  no_hp          String @db.VarChar(16)
  alamat         String
  foto           String
  plain_password String @db.VarChar(255)
  password       String @db.VarChar(255)
  id_tim         Int

  @@index([id_tim], map: "mahasiswa_id_tim")
}

model migrations {
  id        BigInt @id(map: "pk_migrations") @default(autoincrement())
  version   String @db.VarChar(255)
  class     String @db.VarChar(255)
  group     String @db.VarChar(255)
  namespace String @db.VarChar(255)
  time      Int
  batch     Int
}

model posisi_pcl {
  nim          String    @id(map: "pk_posisi_pcl") @db.VarChar(9)
  lokus        String?   @db.VarChar(256)
  latitude     Float?
  longitude    Float?
  akurasi      Float?
  time_created DateTime? @db.Timestamp(6)
}

model riwayatposisipcl {
  id        Int      @id(map: "pk_riwayatposisipcl") @default(autoincrement())
  nim       String   @db.VarChar(9)
  latitude  Float
  longitude Float
  akurasi   Float
  time      DateTime @default(dbgenerated("'2024-01-05 00:00:00'::timestamp without time zone")) @db.Timestamp(6)
}

model rumahtangga {
  kodeRuta       String @id(map: "pk_rumahtangga") @db.VarChar(20)
  no_segmen      Int
  no_bg_fisik    Int
  no_bg_sensus   Int
  no_urut_rt     Int
  nama_krt       String @db.VarChar(255)
  alamat         String
  no_bs          String @db.VarChar(4)
  is_genz_ortu   String @db.VarChar(1)
  jml_genz       Int
  no_urut_rt_egb Int
  long           Float?
  lat            Float?
  catatan        String
}

model timpencacah {
  id_tim   Int    @default(autoincrement())
  nama_tim String @db.VarChar(255)
  nim_pml  String @id(map: "pk_timpencacah") @db.VarChar(9)
}

model token_ambil_sampel {
  riset Int     @id(map: "pk_token_ambil_sampel")
  token Int
  lokus String? @db.VarChar(16)
  count Int
}

model users {
  id       Int    @id(map: "pk_users") @default(autoincrement())
  email    String @db.VarChar(100)
  password String @db.VarChar(200)
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model webmon_log {
  email String? @db.VarChar(255)
  waktu String? @db.VarChar(255)

  @@ignore
}
